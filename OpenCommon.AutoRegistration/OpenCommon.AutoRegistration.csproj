<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Kody Crossman</Authors>
    <Description>Auto Registration of dependencies</Description>
    <PackageId>kcrossman.OpenCommon.AutoRegistration</PackageId>
    <PackageProjectUrl>https://github.com/kcrossman/OpenCommon</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/kcrossman/OpenCommon/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/kcrossman/OpenCommon</RepositoryUrl>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>1</WarningLevel>
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="1.1.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Interfaces\" />
  </ItemGroup>

</Project>
